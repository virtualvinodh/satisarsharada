feature akhn {
# Akhand

lookup akhn1 {
sub shrd-ka @char.virama shrd-ssa by kssa.alt;
sub shrd-ja @char.virama shrd-nya by jnya;
sub shrd-ra @char.virama shrd-kha by rkha;
