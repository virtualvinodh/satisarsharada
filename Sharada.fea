sub shrd-ra @char.virama shrd-nna by rnna;
sub shrd-ra @char.virama shrd-ya by rya;
sub shrd-ra @char.virama shrd-va by rva;

sub shrd-jihva shrd-ka by xka;
sub shrd-jihva shrd-kha by xkha;

} akhn1;

lookup akhn2 {
	sub rja @char.virama shrd-nya by rjnya;
}	akhn2;

lookup psts3 {
    
lookupflag IgnoreMarks;
sub shrd-vs-i shrd-extr-short by shrd-vs-i.short;

} psts3;

lookup akhn1;
lookup akhn2;
