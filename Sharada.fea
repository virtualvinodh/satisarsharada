#> feature
#< feature
} akhn;









































feature psts {

    #Note char.top doesn't exist at this point, use shrd-char

    sub shrd-ka' [shrd-vs-u shrd-vs-uu shrd-vs-vocr shrd-vs-vocrr] by ka.alt;
    sub shrd-nna' [shrd-vs-u shrd-vs-uu shrd-vs-vocr shrd-vs-vocrr] by nna.top;

    sub [shrd-gha shrd-nga shrd-nya shrd-ta shrd-da shrd-pa shrd-ha shrd-ssa shrd-ma shrd-bha shrd-nna kssa.top xka] shrd-virama' by virama.alt;

    sub shrd-sha ma.sub shrd-vs-ii' by vs-ii-alt;
    sub [sa.sub gha.sub ma.sub pa.sub pha.sub da.sub ha.sub dda.sub nna.sub ssa.sub] shrd-vs-ii' by vs-ii-alt;
    sub [sa.sub.short gha.sub.short ma.sub.short pa.sub.short pha.sub.short da.sub.short ha.sub.short dda.sub.short nna.sub.short] shrd-vs-ii' by vs-ii-alt;
	 sub [shrd-ga shrd-dda shrd-nga shrd-ba] ja.sub shrd-vs-ii' by vs-ii-alt;	
    sub shrd-ba [da.sub da.sub.short] vs-ii-alt' by shrd-vs-ii;
    sub shrd-ga bha.sub shrd-vs-ii' by vs-ii-alt;
    sub [shrd-ssa shrd-da shrd-la shrd-tta] [ka.sub ka.alt.sub] shrd-vs-ii' by vs-ii-alt;
    sub shrd-ra [cha.sub bha.sub ssa.sub] shrd-vs-ii' by vs-ii-alt;
    sub shrd-nya [ja.sub jha.sub] shrd-vs-ii' by vs-ii-alt;
    sub shrd-tta cha.sub shrd-vs-ii' by vs-ii-alt;
    sub shrd-ma bha.sub shrd-vs-ii' by vs-ii-alt;
  
    sub [xka xka.alt xkha] shrd-vs-ii' by vs-ii-alt;
    sub [xka xka.alt xkha] shrd-virama' by virama.alt;
    sub [xka xka.alt xkha] shrd-vs-aa' by vs-aa.alt;


    sub shrd-sha ma.sub @char.subs shrd-vs-ii' by vs-ii-alt;
    sub [sa.sub gha.sub ma.sub pa.sub pha.sub da.sub ha.sub dda.sub nna.sub ssa.sub] [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
    sub [sa.sub.short gha.sub.short ma.sub.short pa.sub.short pha.sub.short da.sub.short ha.sub.short dda.sub.short nna.sub.short] [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
	 sub [shrd-ga shrd-dda shrd-nga shrd-ba] ja.sub [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;	
    sub shrd-ba [da.sub da.sub.short] [@char.subs @yara] vs-ii-alt' by shrd-vs-ii;
    sub shrd-ga bha.sub [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
    sub [shrd-ssa shrd-da shrd-la shrd-tta] [ka.sub ka.alt.sub] [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
    sub shrd-ra [cha.sub bha.sub ssa.sub] [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
    sub shrd-nya [ja.sub jha.sub] [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
    sub shrd-tta cha.sub [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;
    sub shrd-ma bha.sub [@char.subs @yara] shrd-vs-ii' by vs-ii-alt;

    lookup psts1 {
       lookupflag IgnoreMarks;
	    sub shrd-nga @char.aa by ngaa;
	    sub shrd-ja @char.aa by jaa;	 
       sub shrd-tta @char.aa by ttaa;
